{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "7.2.*",
    "ext-bcmath": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "bacon/bacon-qr-code": "1.0.3",
    "brick/math": "^0.8.4",
    "fideloper/proxy": "^4.0",
    "fntneves/laravel-transactional-events": "^1.4",
    "ibrunotome/google2fa-laravel": "~1.0.2",
    "jenssegers/agent": "^2.6",
    "laravel/framework": "5.7.*",
    "laravel/horizon": "^2.0",
    "laravel/tinker": "~1.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "owen-it/laravel-auditing": "^8.0",
    "spatie/laravel-cors": "^1.2",
    "spatie/laravel-query-builder": "^1.9",
    "sudiptpa/ipstack": "^1.3",
    "swooletw/laravel-swoole": "^2.5.0",
    "tymon/jwt-auth": "^1.0.0-rc.3"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "Preferred\\": "app/"
    },
    "classmap": [
      "database/seeds"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-install-cmd": [
      "rm -rf bootstrap\/cache/*",
      "chmod -R 755 bootstrap\/cache",
      "php artisan route:clear",
      "php artisan route:cache",
      "php artisan config:clear",
      "php artisan config:cache"
    ],
    "test": "sed -i '$ d' /opt/php72/lib/php.ini && phpunit --no-coverage"
  }
}
